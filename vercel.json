{
    "builds" : [{
        "src":"./index.js",
        "use":"@vercel/node"
    }],
    "outputDirectory": "public",
    "routes":[
        {"src":"/(.*)", "dest": "/public/index.html"},
        {"src":"/", "dest": "/public/$1"}
    ],
    "rewrites": [
    {
      "source": "/_next/live",
      "destination": "/"
    }
  ]
}